<div id="wrapperu">
	<br>
	<div id="search_bar">
	
	<%= render partial: 'shared/search' %>
	

	</div>
	<br>
	<div id="main_show">
		<div id="project_title">
			<% if user_signed_in? and @project %>
				<%= content_tag :h1, best_in_place(@project, :title, :path => user_project_path(current_user, @project.id)) %> 
			<% end %>
	    </div>
		<br> 
		
		<div id="video">
			<ul id="vids"> 
			<% i = 1 %>
					<% @main.each do |video| %>
				<%=	"<li id='videos_#{i}'><iframe width='400' height='250' src='//www.youtube.com/embed/#{video[:id]}?rel=0' frameborder='0'></iframe><p>Click to use this video</p><br><br></li>".html_safe %>  
				<% i += 1 %>
				<% end %>
		</ul>
		</div>
	</div>

</div>